
<div id="wrapper">
<div id="show-block">
<div id="page-content-wrapper">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
<%= search_form_for @search do |f|  %>
<%= f.label :title_or_user_name_cont, "Search collections"  %>
<%= f.text_field :title_or_user_name_cont  %>
<%= f.submit "Search"  %>
<% end %>

<br>

<% @collections.each do |collection| %>
<%= link_to collection.title, collection %>
<% collection.songs.each do |song| %>
<%= song.title %>
<% end %>
<% end %>

<%#= will_paginate @songs %>
            </div>
        </div>
    </div>
</div>
</div>
</div>
