<div id="wrapper">
<div id="show-block">
<div id="page-content-wrapper">
            <div class="col-lg-12">
            <div class="container">
            <div class="row">
            <% @gigs.each do |gig| %>
            
            <div class="col-md-3">
            <div class="panel panel-default" >
              <div class="panel-heading">
                <h3 class="panel-title"><%= link_to gig.name, gig %> -  <%= gig.starting_time %></h3>
              </div>
              <div class="panel-body">
            <div class="row"  position="relative">
            <%= image_tag "http://maps.google.com/maps/api/staticmap?size=450x300&sensor=false&zoom=16&markers=#{gig.latitude}%2C#{gig.longitude}" %>
            </div>
              </div>
              <div class="panel-footer">
              <%= gig.address %>
              </div>
            </div>
            </div>

            <% end %>
            </div>
            </div>
</div>
</div>
</div>
</div>
</div>
